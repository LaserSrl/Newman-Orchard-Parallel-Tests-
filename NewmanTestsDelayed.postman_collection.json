							"var message = messageDiv.text();",
							"//console.log(messageDiv.text());",
							"var testResult = message.indexOf(\"has been created.\") != -1;",
							"tests[\"Creation OK\"] = testResult;",
							"testResult = testResult & responseCode.code === 200;",
